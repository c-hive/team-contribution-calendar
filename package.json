{
  "name": "@c-hive/team-contribution-calendar",
  "version": "0.1.0",
  "description": "GitHub-like contribution calendar for the whole team. Supports GitHub and GitLab.",
  "main": "./lib/index.js",
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib --copy-files",
    "clean:lib": "rimraf lib",
    "lint": "eslint src",
    "test": "nyc --reporter=html --reporter=text mocha src/ --require ./src/utils/TestUtils/mockNonJsResources.js --require @babel/register --require @babel/polyfill --recursive",
    "test:only": "mocha src/ --require ./src/utils/TestUtils/mockNonJsResources.js --require @babel/register --require @babel/polyfill --recursive",
    "prepublishOnly": "npm run clean:lib && npm run lint && npm run test && npm run build",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c-hive/team-contribution-calendar.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c-hive/team-contribution-calendar/issues"
  },
  "homepage": "https://github.com/c-hive/team-contribution-calendar#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@babel/runtime": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-inline-svg": "^1.0.0",
    "babel-preset-minify": "^0.5.0",
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "mocha": "^6.1.2",
    "mocha-jsdom": "^2.0.0",
    "npm": "^6.9.0",
    "nyc": "^13.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.0",
    "proxyquire": "^2.1.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.1",
    "uglify-js": "^3.5.4"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "elly": "^1.1.10",
    "svgson": "^3.1.0"
  }
}
