{
  "name": "team-contribution-calendar",
  "version": "1.0.0",
  "description": "GitHub-like contribution calendar for the whole team. Supports GitHub and GitLab.",
  "main": "./lib/index.js",
  "scripts": {
    "build": "BABEL_ENV=production babel src --out-dir lib",
    "clean:lib": "rimraf lib",
    "lint": "eslint src",
    "test": "nyc mocha src/ --require @babel/register --require @babel/polyfill --recursive",
    "test:only": "mocha src/ --require @babel/register --require @babel/polyfill --recursive",
    "prepublishOnly": "npm run clean:lib && npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c-hive/team-contribution-calendar.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c-hive/team-contribution-calendar/issues"
  },
  "homepage": "https://github.com/c-hive/team-contribution-calendar#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-preset-minify": "^0.5.0",
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "install": "^0.12.2",
    "mocha": "^6.1.2",
    "npm": "^6.9.0",
    "nyc": "^13.3.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3",
    "uglify-js": "^3.5.4"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
